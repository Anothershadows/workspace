package javaqa;

public class patern {

	public static void main(String[] args) {
		System.out.println("start");
		for(int i = 1; i<=5; i++) {
			
			for(int j = 1; j<=5-i; j++) {
				System.out.print(" ");
			}
			for(int k = 1; k<=i; k++) {
			System.out.println("*");
			}
			System.out.println();
		}
				
	}
}

