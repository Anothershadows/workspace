package javaqa;

public class patern {

	public static void main(String[] args) {
		System.out.println("start");
		for(int i = 1; i<=5; i++) {
			
			for(int j = i; j<=5; j++) {
				System.out.print(" ");
			}
			
			for(int j = i; j<=5; j++) {
				System.out.println("1");
				System.out.println("*");
			}
		}
				
	}

}
